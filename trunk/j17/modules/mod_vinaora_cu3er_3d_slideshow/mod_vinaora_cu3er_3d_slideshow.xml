				<field name="slide_description_paragraph" type="textarea" default="" label="MOD_VC3S_FIELD_SLIDE_DESCRIPTION_PARAGRAPH" description="MOD_VC3S_FIELD_SLIDE_DESCRIPTION_PARAGRAPH_DESC" rows="5" cols="30" ></field>
