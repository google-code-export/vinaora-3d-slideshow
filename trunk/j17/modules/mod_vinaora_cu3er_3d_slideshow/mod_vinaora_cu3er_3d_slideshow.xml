					<option value="1">JYES</option>
					<option value="0">JNO</option>
				</field>
				<field name="next_symbol_type" type="list" default="3" label="MOD_VC3S_FIELD_NEXT_SYMBOL_TYPE" description="MOD_VC3S_FIELD_NEXT_SYMBOL_TYPE_DESC" >
